
(
    echo '(function (global) {'
    closure --process_common_js_modules \
        --common_js_entry_module collections.js \
        --js \
            abstract-map.js \
            array-shim.js \
            array.js \
            collections.js \
            dict.js \
            fast-map.js \
            fast-set.js \
            iterator.js \
            list.js \
            lru-map.js \
            lru-set.js \
            map.js \
            multi-map.js \
            object.js \
            observable.js \
            reducible.js \
            set.js \
            sorted-map.js \
            sorted-set.js \
            tree-log.js \
            weak-map.js \

    echo '})(this);'
) > collections.cat.js

closure < collection.cat.js > collections.min.js

wc -c collections.min.js
cat collections.min.js | gzip | wc -c


